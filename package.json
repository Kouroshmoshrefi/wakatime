{
  "name": "wakatime-bot",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc && shx cp -r src/api/web/ dist/api/",
    "dev": "node --watch --loader tsm ./src/main.ts",
    "postinstall": "npm run build",
    "start": "node dist/main.js",
    "prepare": "husky install",
    "lint": "npm-run-all -p lint:*",
    "lint:style": "stylelint \"src/api/web/css/*.css\"",
    "lint:es": "eslint . --fix"
  },
  "dependencies": {
    "@fullstacksjs/eslint-config": "8.9.0",
    "@fullstacksjs/toolbox": "3.0.2",
    "@fullstacksjs/tsconfig": "1.0.0",
    "@grammyjs/ratelimiter": "1.1.6",
    "@types/cron": "1.7.3",
    "@types/express": "4.17.14",
    "@types/node": "18.11.9",
    "awilix": "8.0.0",
    "axios": "0.22.0",
    "cron": "1.8.2",
    "deepmerge-ts": "4.2.2",
    "dotenv": "16.0.3",
    "dotenv-cli": "7.0.0",
    "ejs": "3.1.8",
    "eslint": "8.28.0",
    "express": "4.18.2",
    "grammy": "1.12.0",
    "husky": "8.0.2",
    "lint-staged": "12.0.2",
    "lowdb": "3.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.0",
    "puppeteer": "13.1.1",
    "shx": "0.3.4",
    "stylelint": "15.6.1",
    "stylelint-config-standard": "33.0.0",
    "ts-dedent": "2.2.0",
    "tsm": "2.3.0",
    "typescript": "4.9.4"
  },
  "volta": {
    "node": "18.13.0",
    "npm": "9.4.0"
  }
}
